{
  "imports": {
    "std/assert": "https://deno.land/std@0.214.0/assert/mod.ts"
  },
  "fmt": {
    "exclude": ["npm/"],
    "singleQuote": true
  },
  "lint": {
    "exclude": ["npm/"],
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "tasks": {
    "node": "deno run --allow-env --allow-read --allow-write=./npm --allow-net=deno.land --allow-run scripts/build_npm.ts"
  }
}
